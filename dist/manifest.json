{
  "manifest_version": 2,
  "name": "friendly",
  "version": "1.0.0",

  "icons": {
    "128": "16.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "content.js" ]
    }
  ],
  "browser_action": {
        "default_title": "friendly",
        "default_icon": "16.png",
        "default_popup": "popup.html"
  }
}
